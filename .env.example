# configure the following for Azure
API_TYPE="azure"
AZURE_API_VERSION=
AZURE_API_URL=
AZURE_API_KEY=

# for openai (uncomment API_TYPE="openai" and set OPENAI_API_KEY)
# API_TYPE="openai"
OPENAI_API_KEY=
OPENAI_API_URL="https://api.openai.com/"

# for local vLLM with dual-model support
# Set API_TYPE="openai" and configure base URLs for each model
# Model 1 uses OPENAI_API_BASE, Model 2 uses OPENAI_API_BASE_2
OPENAI_API_BASE=http://localhost:8000/v1
OPENAI_API_BASE_2=http://localhost:8001/v1

# for gemini
GEMINI_API_KEY=

# for groq
GROQ_API_KEY=
